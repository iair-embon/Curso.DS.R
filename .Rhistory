reglacorte <- function(x,t){
v <- c()
for (i in 1:length(t)) {
y_predicted <- ifelse(x >= t[i],1,0)
error <- ifelse(y_predicted == datos$especie,0,1)
r <- sum(error)/nrow(datos)
v <- c(v,r)
}
return(v)
}
