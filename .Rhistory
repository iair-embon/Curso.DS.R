# Leo los datos
df <- read.table("http://astrostatistics.psu.edu/datasets/GRB_afterglow.dat",
header=T, skip=1)
require(dplyr)
### 1
## P(X<=40)
prob_acum_40 <- df %>%
filter(f <=40) %>%
nrow()/nrow(df)
## standar error
# busco el valor
quantile(df$f, probs =  prob_acum_40)
# parece que no esta este valor en el vector
sum(df$f == 40)
# busco el valor mas cercano a 40 en el vector
index <- which.min(abs(df$f - 40))
q66 <- df$f[index]
# ahora busco el error standar asociado
q66_es <- df$s[index]
### 2
prom_muestra <- mean(df$f)
es_muestra <- sd(df$f)/ sqrt(nrow(df))
