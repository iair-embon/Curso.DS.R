ClasificoGenerativo <- function(X, Y, xNuevo){
df <- data.frame(genero = Y,
altura = X)
prop_f <- df %>%
filter(genero == "F" &
altura > (xNuevo-1) &
altura < (xNuevo+1)) %>%
nrow()/nrow(df)
prop_m <- df %>%
filter(genero == "M" &
altura > (xNuevo-1) &
altura < (xNuevo+1)) %>%
nrow()/nrow(df)
regla <- ifelse(prop_f>prop_m,1,0)
return(regla)
}
